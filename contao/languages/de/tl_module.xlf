    <file source-language="en" target-language="de" datatype="plaintext" original="tl_module">
        <body>
            <!-- Legend -->
            <trans-unit id="tl_module.whatsapp_legend">
                <source>WhatsApp Settings</source>
                <target>WhatsApp-Einstellungen</target>
            </trans-unit>

            <!-- Fields -->
            <trans-unit id="tl_module.whatsappTitle.0">
                <source>WhatsApp Title</source>
                <target>WhatsApp-Titel</target>
            </trans-unit>
            <trans-unit id="tl_module.whatsappTitle.1">
                <source>Enter the title for the WhatsApp module.</source>
                <target>Geben Sie den Titel für das WhatsApp-Modul ein.</target>
            </trans-unit>
            <trans-unit id="tl_module.whatsappIsVisible.0">
                <source>Display WhatsApp Button</source>
                <target>WhatsApp-Schaltfläche anzeigen</target>
            </trans-unit>
            <trans-unit id="tl_module.whatsappIsVisible.1">
                <source>Show the WhatsApp contact button on the page if enabled.</source>
                <target>Zeigt die WhatsApp-Kontaktschaltfläche auf der Seite an, wenn sie aktiviert ist.</target>
            </trans-unit>
            <trans-unit id="tl_module.whatsappNumber.0">
                <source>WhatsApp Number</source>
                <target>WhatsApp-Nummer</target>
            </trans-unit>
            <trans-unit id="tl_module.whatsappNumber.1">
                <source>Enter the phone number for WhatsApp (e.g., 1234567890).</source>
                <target>Geben Sie die Telefonnummer für WhatsApp ein (z. B. 1234567890).</target>
            </trans-unit>

            <trans-unit id="tl_module.whatsappMessage.0">
                <source>WhatsApp Message</source>
                <target>WhatsApp-Nachricht</target>
            </trans-unit>
            <trans-unit id="tl_module.whatsappMessage.1">
                <source>Enter the default message to be sent via WhatsApp.</source>
                <target>Geben Sie die Standardnachricht ein, die über WhatsApp gesendet werden soll.</target>
            </trans-unit>
        </body>
